{
    "telemetry.telemetryLevel": "off",
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.startupEditor": "none",
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "editor.fontFamily": "'Source Code Pro for Powerline', 'Consolas, DejaVu Sans Mono', monospace",
    "editor.fontSize": 13,
    "editor.fontLigatures": "'ccmp' off",
    "editor.cursorWidth": 2,
    "editor.cursorSurroundingLines": 10,
    "editor.renderWhitespace": "all",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.formatOnPaste": true,
    "editor.comments.ignoreEmptyLines": false,
    "editor.definitionLinkOpensInPeek": true,
    "editor.guides.bracketPairs": "active",
    "editor.renderLineHighlight": "gutter",
    "editor.linkedEditing": true,
    "editor.multiCursorMergeOverlapping": false,
    "editor.parameterHints.cycle": true,
    "editor.scrollbar.verticalScrollbarSize": 10,
    "editor.scrollbar.horizontalScrollbarSize": 10,
    "editor.stickyTabStops": true,
    "editor.suggest.preview": true,
    "editor.minimap.side": "left",
    "editor.minimap.showSlider": "always",
    "editor.minimap.maxColumn": 80,
    "editor.minimap.renderCharacters": false,
    "editor.semanticHighlighting.enabled": false,
    "editor.tokenColorCustomizations": {
        "[One Dark Pro Darker]": {
            "textMateRules": [
                {
                    "scope": "markup.fenced_code.block.markdown",
                    "settings": {
                        "foreground": "#b1e18e"
                    }
                },
                {
                    "scope": "source.python",
                    "settings": {
                        "foreground": "#e2e4e9"
                    }
                },
                {
                    "scope": "meta.attribute.python",
                    "settings": {
                        "foreground": "#f58992"
                    }
                }
            ]
        },
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "diffEditor.codeLens": true,
    "explorer.incrementalNaming": "smart",
    "files.simpleDialog.enable": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
    "search.seedOnFocus": true,
    "search.smartCase": true,
    "search.showLineNumbers": true,
    "scm.alwaysShowRepositories": true,
    "extensions.autoUpdate": false,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.letterSpacing": 1,
    "terminal.integrated.allowChords": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.detectLocale": "off",
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.scrollback": 5000,
    "terminal.integrated.tabs.location": "left",
    "keyboard.touchbar.enabled": false,
    "atomKeymap.promptV3Features": true,
    "markdown.extension.list.indentationSize": "inherit",
    "markdown.extension.tableFormatter.normalizeIndentation": true,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "aws.telemetry": false,
    "aws.profile": "profile:default",
    "git.autofetch": true,
    "gitlens.views.commits.avatars": false,
    "redhat.telemetry.enabled": false,
    "material-icon-theme.saturation": 0.75,
    "oneDarkPro.color": {
        "chalky": "#e2e4e9",
        "coral": "#f58992",
        "green": "#b1e18e",
        "purple": "#c678dd",
        "fountainBlue": "#6ad4e2",
        "malibu": "#61afef",
        "whiskey": "#ecb37e",
        "lightDark": "#9ea3ae",
        "lightWhite": "#f0f2f4",
        "dark": "#505662",
        "deepRed": "#ff0000",
        "error": "#00ff00",
        "invalid": "#0000ff"
    },
    "json.schemas": []
}